package mediaone;

/**
 *
 * @author Admin
 */
public class Store {
    private int monthlyCost;
    private int revenue;

    public void setMonthlyCost(int monthlyCost) {
        this.monthlyCost = monthlyCost;
    }

    public void setRevenue(int revenue) {
        this.revenue = revenue;
    }

    public int getMonthlyCost() {
        return monthlyCost;
    }

    public int getRevenue() {
        return revenue;
    }
    
}
